set nobackup
set noswapfile

syntax on
colorscheme molokai
set t_Co=256
set number
set ruler
"set autoindent
set expandtab
set tabstop=2
set fileencodings=iso-2022-jp,euc-jp,utf-8,sjis
set fileformats=unix,dos,mac
set noautoindent
set browsedir=~/Document/

set lines=40
set columns=120

NeoBundle 'nathanaelkane/vim-indent-guides'

colorscheme morning
